
<?xml version="1.0"?>
<CLBConfig version="2.0" output="output/">
      <Geometry nx="500" ny="100" >
      <CM><Box /></CM>    
      <!-- <Smoothing><Box /></Smoothing>  -->
      
        <None name="init_T_region">
            <Sphere dx="150" nx="50" dy="25" ny="50" /> 
        </None>        

        <Wall mask="ALL" name="box" >     
           <Box dx="100" dy="25" nx="20" ny="25"/>
        </Wall>
        <DarcySolid name="darcy_solid_region">
          <Box dx="100" dy="50" nx="20" ny="25"/>
        </DarcySolid>


        <HeaterDirichletTemperature name="dirichlet_fluid_region"> 
          <Box dx="200" dy="50" nx="20" ny="25"/>
        </HeaterDirichletTemperature>
        

        <Wall mask="ALL" name="box" >     
           <Box dx="300" dy="10" nx="25" ny="25"/>
        </Wall>
         <HeaterDirichletTemperature name="dirichlet_wall_region"> 
          <Box dx="300" dy="10" nx="25" ny="25"/>
        </HeaterDirichletTemperature>

      	<DarcySolid name="darcy_solid_region">
      	  <Box dx="400" dy="60" nx="25" ny="25"/>
      	</DarcySolid>
        <HeaterDirichletTemperature name="dirichlet_darcy_solid_region"> 
          <Box dx="400" dy="60" nx="25" ny="25"/>
        </HeaterDirichletTemperature>


         <HeaterNeumannHeatFlux  name="neumann_heater_region"> 
          <Box dx="300" dy="25" nx="20" ny="25"/>
        </HeaterNeumannHeatFlux>
        <HeaterNeumannHeatFlux  name="neumann_cool_region"> 
          <Box dx="350" dy="50" nx="20" ny="25"/>
        </HeaterNeumannHeatFlux>
  </Geometry>
    <Model>
    <Params
        k="0.1666666" 
        cp="1.0"


        InitTemperature="0.0"
        InitTemperature-init_T_region="-10"

        InitTemperature-dirichlet_wall_region="10.0"
        InitTemperature-dirichlet_fluid_region="-10.0"
        InitTemperature-dirichlet_darcy_solid_region="10.0"

        

        InitTemperature-neumann_heater_region="1E0"
        InitTemperature-neumann_cool_region="-1E0"
        />
    <Params  />
    <Params GravitationX="-1.0E-5" />
    <!-- <Params GravitationY="-1.0E-7" /> -->
    <!-- <Params BoussinesqCoeff="1"/> -->
    <!-- <Params VelocityX="-1.0E-1" /> -->

    </Model> 
