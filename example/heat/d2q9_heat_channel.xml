<Solve Iterations="10" output="output/"> <VTK Iterations="1"/> </Solve>
<Failcheck Iterations="10" nx="500" ny="100" />
<Log Iterations="1000"/>
<Solve Iterations="20000" output="output/"> <VTK Iterations="250"/> </Solve>
</CLBConfig>

