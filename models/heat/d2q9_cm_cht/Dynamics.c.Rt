	temp[8] = ux2*uy2*h_in[0] + 2.*ux2*u.y*h_in[2] + ux2*h_in[4] + 2.*u.x*uy2*h_in[1] + 4.*uxuy*h_in[5] + 2.*u.x*h_in[7] + uy2*h_in[3] + 2.*u.y*h_in[6] + h_in[8];
	//back to density-probability functions
	h_in[0] = temp[0] - temp[3] - temp[4] + temp[8];
