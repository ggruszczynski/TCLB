	real_t H = h_in[0] + h_in[1] + h_in[2] + h_in[3] + h_in[4] + h_in[5] + h_in[6] + h_in[7] + h_in[8];

	real_t Sigma2 = (h_stability_enhancement*1./3.)/(cp*rho);

	real_t temp[9];
	for (int i = 0; i < 9; i++) {
		temp[i] = h_in[i];}
	//raw moments from density-probability functions
