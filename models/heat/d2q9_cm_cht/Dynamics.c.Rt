   ({ __typeof__ (a) _a = (a); \
       __typeof__ (b) _b = (b); \
     _a > _b ? _a : _b; })

CudaDeviceFunction void EConvect()
{
	real_t U_local = myMax(0, U(-1,0,0)); // TODO: anty-cofka?
	real_t invU = 1.0/(1+ U_local);

	<?R
		if (Options$OutFlow) {
			C(f, (f_old + U_loc*f_n)*U_inv)
			C(f_old, f)

			C(h, (h_old + U_loc*h_n)*U_inv)
			C(h_old, h)
		}
	?>
}

