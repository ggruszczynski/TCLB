	real_t Sigma2 = (h_stability_enhancement*1./3.)/(cp*rho);

	real_t temp[<?R C( h_pop_size) ?>];

	for (int i = 0; i < <?R C( h_pop_size) ?>; i++) {
		temp[i] = x_in[i];}

